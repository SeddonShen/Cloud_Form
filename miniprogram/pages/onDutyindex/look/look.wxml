<view class="padding text-center">
  <view class="padding-xl radius shadow bg-white text-xl">
    <view class="text-xxl padding-bottom"><text class="text-bold" style="color:var(--brown);">排班管理</text></view>
    {{team}}
    <view wx:if="{{!groupdate.length==0}}" class="cu-list menu card-menu padding-top">
      <navigator style="display: flex; align-items: center;margin: 15rpx 0;border-bottom: 1rpx solid #ddd;justify-content: space-between;" wx:for="{{groupdate}}" url="/pages/onDutyindex/look/detail/detail?_id={{item._id}}" wx:key="index">

        <view class='cu-item content title' style="margin-right: 15rpx;">
          <text wx:if="{{!item.progress}}" class="cuIcon-check lg text-green"></text>
          <text wx:else class="cuIcon-activityfill lg text-brown"></text>
          <!-- <text class="text-888  text-lg text-left">{{item.datesend}} {{item.time_begin}} - {{item.time_end}}</text>

          <view style='transform:rotate(0deg);transition:all 0.4s;'></view> -->
        </view>
        <view class="text-888 text-lg text-left">
          <view>名称：{{!item.activeName?'未命名':item.activeName}}</view>
          <view>地点：{{!item.place?'未选择地点':item.place}}</view>
          <view>{{item.myDate}} {{item.time_begin}} - {{item.time_end}}</view>
        </view>
        <view class='cu-item content title'>
          <text class="cuIcon-right lg text-gray"></text>
        </view>
      </navigator>

    </view>
    <view wx:else class="padding-xl text-xxl">暂无排班任务</view>
  </view>
</view>